# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/index/{index}							Application.index
GET    	/                                       Application.index

#SimpleQuadroTest
GET			/quadro/setup						SimpleQuadroController.setup
POST		/quadro/setup						SimpleQuadroController.setTable
POST		/quadro/graph						SimpleQuadroController.calcQuadroGraph

#CNA Tests
GET			/cna/setup							CNAController.setup
POST		/cna/setup							CNAController.generateGraph
POST /cna/setup/graph					CNAController.calcCNAGraph


GET		/testing/{step} 						Application.complexTesting



# Map static resources from the /app/public folder to the /public path

GET     /public/                                staticDir:public



# Catch all
*       /{controller}/{action}                  {controller}.{action}


#/testing/{step}/{showTable}/{table}/{generatedGraphPath}/{generatedGraph}/{calculatedGraphPath}/{calculatedGraph}/{time}/{algorithmName}