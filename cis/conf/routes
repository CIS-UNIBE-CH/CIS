# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/index/{index}							Application.index
GET    	/                                       Application.index

GET		/contact								Application.contact
GET		/disclaimer								Application.disclaimer

#SimpleQuadroTest
GET			/quadro/setup						QuadroTestController.setup
POST		/quadro/setup						QuadroTestController.setTable
POST		/quadro/graph						QuadroTestController.calcQuadroGraph

#CNA Tests
GET			/cna/setup/1						CNAController.setup
POST		/cna/setup/2						CNAController.prepare
POST		/cna/setup							CNAController.generateGraph
POST		/cna/graph							CNAController.calcCNAGraph
POST		/cna/mtinput/setup/2				CNAController.inputMT
POST		/cna/tableinput/graph				CNAController.inputTable
POST		/cna/quickrun/graph					CNAController.baumgartnerSample

#CNA Tests
GET			/input/setup						Input.setup
POST		/input/graph						Input.calcCNAGraph
GET			/draw/setup							DrawGraph.setup
POST		/draw/graph							DrawGraph.drawGraph

GET		/testing/{step} 						Application.complexTesting



# Map static resources from the /app/public folder to the /public path

GET     /public/                                staticDir:public



# Catch all
*       /{controller}/{action}                  {controller}.{action}


#/testing/{step}/{showTable}/{table}/{generatedGraphPath}/{generatedGraph}/{calculatedGraphPath}/{calculatedGraph}/{time}/{algorithmName}